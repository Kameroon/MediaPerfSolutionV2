             xmlns:local="clr-namespace:MediaPerf.Application"
             StartupUri="pack://application:,,,/MediaPerf.Modules.Application.Core;component/Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary     
                    Source="pack://application:,,,/MediaPerf.Framework.Application;component/Resources/Authers.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/MediaPerf.Framework.Application;component/Styles/Texts.xaml" />
                <ResourceDictionary
                    Source="pack://application:,,,/MediaPerf.Framework.Application;component/Styles/Panels.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
